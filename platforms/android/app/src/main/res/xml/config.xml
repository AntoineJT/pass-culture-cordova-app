<?xml version='1.0' encoding='utf-8'?>
<widget id="com.github.antoinejt.passculture" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Pass Culture</name>
    <description>
        Une application cordova toute simple pour accéder facilement à l'app web du pass culture 
    </description>
    <author email="tournepiche.antoine@gmail.com" href="https://github.com/AntoineJT">
        Antoine "AntoineJT" Tournepiche
    </author>
    <content src="index.html" />
    <icon src="logo.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
